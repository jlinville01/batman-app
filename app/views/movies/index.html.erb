<h1>Movies</h1>

<table>
  <thead>
  <tr>
    <th>Title</th>
    <th>Release Date</th>
    <th>Synopsis</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @movies.each do |movie| %>
    <tr>
      <td><%= movie.title %></td>
      <td><%= movie.release_date %></td>
      <td><%= movie.synopsis %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br><br><%= link_to 'Home Page', root_path %>